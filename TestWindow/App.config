<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <appSettings>
    <add key ="DBFormat" value ="SQLite" />
    <add key="DeliveriesCountMin" value="3" />
    <add key="DeliveriesCountMax" value="6" />
    <add key="TaskIntervalMin" value="10" />
    <add key="TaskIntervalMax" value="20" />
    <add key="ExpirationTime" value="5" />
  </appSettings>
  <connectionStrings>
    <add name="XMLFile" connectionString="D:\HotDeliveryDB.XML"/>
    <add name="SQLite" connectionString="D:\HotDeliveryDB.db"/>
  </connectionStrings>
</configuration>